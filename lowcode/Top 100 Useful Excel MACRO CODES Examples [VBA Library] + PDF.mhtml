    With Rows(=E2=80=9C3:3=E2=80=9D)<br>
        Set Rng =3D .Find(What:=3DFindString, _<br>
                        After:=3D.Cells(.Cells.Count), _<br>
                        LookIn:=3DxlFormulas, _<br>
                        LookAt:=3DxlWhole, _<br>
                        SearchOrder:=3DxlByRows, _<br>
                        SearchDirection:=3DxlNext, _<br>
                        MatchCase:=3DFalse)<br>
